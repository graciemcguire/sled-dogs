<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <%= form_for @dog do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :breed %>
      <%= f.text_field :breed %>
      <%= f.label :age %>
      <%= f.text_field :age %>
      
      <%= f.submit %>
    <% end  %>
  </body>
</html>
